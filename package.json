{
  "name": "react-quill",
  "version": "1.3.4",
  "description": "The Quill rich-text editor as a React component.",
  "author": "zenoamaro <zenoamaro@gmail.com>",
  "homepage": "https://github.com/zenoamaro/react-quill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zenoamaro/react-quill/issues",
    "email": "zenoamaro@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoamaro/react-quill.git"
  },
  "main": "lib/index.js",
  "browser": "lib/index.js",
  "types": "types.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist && npm run build:css",
    "build:lib": "tsc",
    "build:dist": "webpack",
    "build:css": "cp node_modules/quill/dist/quill.{core,snow,bubble}.css dist",
    "watch": "tsc --watch",
    "clean": "rm -rf lib dist",
    "test": "npm run test:unit && npm run test:coverage",
    "test:unit": "mocha --recursive --require=./test/setup.js -R spec test/index",
    "test:coverage": "mocha --recursive --require=./test/setup.js -R mocha-text-cov test/index",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">= 0.8.x"
  },
  "keywords": [
    "react",
    "react-component",
    "rich",
    "text",
    "rich-text",
    "textarea",
    "quill"
  ],
  "files": [
    "dist/",
    "lib/",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "dependencies": {
    "react-dom": "^15.6.1",
    "react": "^15.6.1",
    "create-react-class": "^15.6.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "quill": "^1.3.7",
    "react-dom-factories": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/create-react-class": "^15.6.3",
    "@types/lodash": "^4.14.146",
    "@types/prop-types": "^15.7.3",
    "@types/quill": "^1.3.7",
    "@types/react-dom-factories": "^1.0.2",
    "@types/react-dom": "^15.5.11",
    "@types/react": "^15.6.1",
    "blanket": "^1.2.3",
    "chai-enzyme": "^0.8.0",
    "chai": "^4.1.0",
    "cheerio": "^1.0.0-rc.1",
    "enzyme": "^2.9.1",
    "jsdom-global": "^3.0.2",
    "jsdom": "^11.0.0",
    "mocha-text-cov": "^0.1.1",
    "mocha": "^3.4.2",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "should": "^4.3.0",
    "sinon": "^2.3.5",
    "travis-cov": "^0.2.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
